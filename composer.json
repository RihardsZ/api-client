{
    "name": "elektrum/soap-client",
    "description": "SOAP analogue of Laravel HTTP client",
    "keywords": [
        "elektrum",
        "laravel",
        "soap-client"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Rihards Zbitkovskis",
            "email": "rihards.zbitkovskis@cubesystems.lv",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "ext-soap": "*",
        "ext-sodium": "*",
        "ext-xsl": "*",
        "codedredd/laravel-soap": "^3.0",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Elektrum\\SoapClient\\": "src",
            "Elektrum\\SoapClient\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elektrum\\SoapClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elektrum\\SoapClient\\SoapClientServiceProvider"
            ],
            "aliases": {
                "SoapClient": "Elektrum\\SoapClient\\Facades\\SoapClient"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
